{
  "version": 2,
  "framework": null,
  "builds": [
    {
      "src": "/api/app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "maxDuration": 10,
        "memory": 256
      }
    },
    {
      "src": "/static/**",
      "use": "@vercel/static"
    },
    {
      "src": "/templates/**",
      "use": "@vercel/static"
    }
  ],
  "outputDirectory": "static",
  "env": {
    "PYTHON_VERSION": "3.9"
  },
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api/app.py"
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
